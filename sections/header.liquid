<style>
  .section-header.shopify-section-group-header-group{
    position: sticky;
    top: 0;
  }
  
  .yas-header
  {
    display: block;
    background-color: transparent;
    display: flex;
    flex-wrap: nowrap;
    top: 0;
    z-index: 1000;
    position: sticky;
    justify-content: space-between;
  }
 .header-logo
  {
    height: 100%;
    width:calc(100% - 149px);
    padding-left: 12px;
    padding-top: 12px;
    padding-bottom: 12px;
    opacity: 0;
 
   mix-blend-mode: difference;
    background: transparent;
    display: flex;
    z-index: 1;
  
    
  }
   .header-logo svg
  {
    cursor: pointer;
    width: 119px;
    height: 24px;
    display: block;
     
    
  }

  .header-right{
    display: flex;
    flex-wrap: nowrap;
    paddin-right: 0px;
    padding-top: 4px;
    padding-bottom: 4px;
  }
  .header-cart
  {

  }
  .header-menu
  {

  }
 
  .yas-menu-f{
    position: fixed;
    width: 350px;
    min-height: 500px;
    
    display: none;
    flex-direction: column;
    right: 0px;
    top: 0;
    z-index: 1001;
    padding: 4px 0 9px;
   /* background: rgba(255,255,255,0.8); */
    background: rgba(255, 255, 255, 0.5);
    backdrop-filter: blur(32px) brightness(1.6) contrast(0.9) saturate(1);
    -webkit-backdrop-filter: blur(32px) brightness(1.6) contrast(0.9) saturate(1);
   }
  {
    right: 69px;
    padding-bottom: 0;
  }
  .yas-menu-f-minicart{
    margin-right: 69px;
  }
  .yas-menu-f-minicart .cart-items{
    max-height: 290px;
    overflow: auto;
    margin-right: 8px;
  }
  .yas-menu-f-minicart .cart-items::-webkit-scrollbar{
    padding-right: 8px;
  }
  .yas-menu-f-textr{
    left: 50%;
    top: 50%;
   transform: translate(-50%,-50%);
    width: 350px;
    height: auto;
    max-height: 500px;
    padding-bottom: 0;
  }
   .yas-menu-f form{
      display: flex;
      flex-direction: column;
      height: 100%;
      min-height: 443px;
    }
 /* .yas-menu-f:after{
    content: '';
    background: linear-gradient(0deg, #3D3D3D 0%, #3D3D3D 100%), rgba(188, 188, 184, 0.75);
    background-blend-mode: color-dodge, normal;
    backdrop-filter: blur(25px);
  } */
  .yas-menuf-bottom{
    margin-top: auto;
    display: flex;
    justify-content: space-between;
    padding: 0 16px;
  }
  .yas-menuf-link{
    display: flex;
    flex-direction: column;
  }
  .yas-menuf-link a{
    color: #181806;
    text-align: center;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px; /* 150% */
    text-transform: uppercase;
    padding: 11px 16px;
    display: inline-flex;
    align-self: flex-start;
    text-decoration: none;
    letter-spacing: 0;
  }
  .yas-menuf-link a:hover, .yas-menuf-bottom a:hover{
    text-decoration: underline;
    text-underline-offset: 3px;
  }
  .yas-menuf-link a:active, .yas-menuf-bottom a:active{
    color: #B8B201;
  }
  .yas-menuf-bottom a{
    color: #181806;
    text-align: center;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px; /* 150% */
    text-transform: uppercase;
    padding: 11px 0px;
    display: inline-flex;
    align-self: flex-start;
    text-decoration: none;
    letter-spacing: -0.025rem;
  }
  .yas-menu-close{
    color: #181806;
    text-align: center;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px; /* 150% */
    text-transform: uppercase;
    padding: 11px 16px;
    display: inline-flex;
    align-self: flex-start;
    text-decoration: none;
    position: absolute;
    right: 0;
    border: none;
    background: transparent;
    outline: none;
    cursor: pointer;
    box-shadow: none;
    -webkit-tap-highlight-color: transparent !important;
  }

  .yas-menu-close:active{
    color: #B8B201;
    -webkit-tap-highlight-color: transparent;
  }
  .yas-menu-close:hover:active span:after{
    background-color: #B8B201;
  }
  .yas-menu-close span{
    position: relative;
  }
  .yas-menu-close:hover span:after{
    content: '';
    position: absolute;
    width: 100%;
    height: 1px;
    background: #000;
    bottom: 0;
    left: 0;
  }
  .menu-btn-yas, .cart-btn-yas{
    border: none;
    background: transparent;
    outline: none;
    cursor: pointer;
    color: #181806;
    text-align: center;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px; /* 150% */
    text-transform: uppercase;
    padding: 11px 16px;
    display: inline-flex;
    
  }
  .menu-btn-yas span, .cart-btn-yas span{
    position: relative;
  }
  .menu-btn-yas:active span, .cart-btn-yas:active span{
    color: #474dfe;
  }
  .menu-btn-yas:hover span:after, .cart-btn-yas:hover span:after{
    content: '';
    position: absolute;
    width: 100%;
    height: 1px;
    background: #fff;
    bottom: 0;
    left: 0;
  }
  .menu-btn-yas:active span:after, .cart-btn-yas:active span:after{
    background: #474dfe;
  }
  .cart-btn-yas[data-count="0"]{
    display: none;
  }
  .yas-menu-f-title{
    padding: 0px 16px 4px;
  }
  .yas-menu-f-title > span{
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px; /* 150% */
    text-transform: uppercase;
    padding: 11px 0;
    display: inline-flex;
    color: #000;
    align-items: center;
  }
  .yas-menuf-text{
    padding: 0 10px;
    overflow: auto;
    margin: 0 6px;
  }
  /* width */
  .yas-menu-f ::-webkit-scrollbar {
    width: 4px;
  }
  
  /* Track */
  .yas-menu-f ::-webkit-scrollbar-track {
    background: transparen; 
    border-radius: 999px;
  }
   
  /* Handle */
  .yas-menu-f ::-webkit-scrollbar-thumb {
    background: #BDBDB9;
    border-radius: 999px;
  }
  
  /* Handle on hover */
  .yas-menu-f ::-webkit-scrollbar-thumb:hover {
    background: #BDBDB9; 
  }
  
  .yas-menuf-text p, .yas-menuf-text li{
    color: #181806;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 20px; /* 166.667% */
    margin-top: 0;
    margin-bottom: 14px;
  }
  .yas-menuf-text p:not(:first-child):last-child, .yas-menuf-text li:not(:first-child):last-child{
    margin-bottom: 0;
  }
  .yas-menuf-item-img img{
    width: 86px;
    height: 86px;
    object-fit: contain;
  }
  .yas-menuf-item{
    display: flex;
    flex-wrap: nowrap;
    height: 86px;
    padding: 0 16px;
    margin-bottom: 16px;
    padding-right: 8px;
  }
  
  .yas-menuf-item-info{
    margin: 2px 12px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
  }
  
  .yas-menuf-item-title{
    color: #181806;
    text-decoration: none;
    text-underline-offset: 5px;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px; /* 150% */
    text-transform: uppercase;
  }
  .yas-menuf-item-title:hover{
    text-decoration: underline;
  }
  .yas-menuf-item-title:hover:active{
    color: #B8B201;
    
  }
  .yas-menuf-item-option{
    color: #878785;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px; /* 150% */
    text-transform: uppercase;
  }
  .yas-menuf-item-qaun{
    width: 114px;
    height: 32px;
    display: flex;
    
  }
  .yas-menuf-item-qaun button{
    border: 1px solid #181806;
    width: 32px;
    height: 32px;
    background: transparent;
    padding: 0px;
    cursor: pointer;
    align-items: center;
    display: flex;
    justify-content: center;
    background-repeat: no-repeat;
    background-position: center;
    position: relative;
  }
  .yas-menuf-item-qaun button span{
    position: relative;
    width: 100%;
    height: 100%;
    align-items: center;
    display: flex;
    justify-content: center;
    z-index: -1;
  }

  .yas-menuf-item-qaun button span:after{
    content: '';
    background: #181806;
    width: 15px;
    height: 1px;
    position: absolute;
  }

  .yas-menuf-item-qaun button.qty-count--add span:before{
    content: '';
    background: #181806;
    width: 15px;
    height: 1px;
    position: absolute;
    transform: rotate(90deg);
  }

  
  .yas-menuf-item-qaun button:hover span{
    background-color: #181806;
  }
  .yas-menuf-item-qaun button:hover span:after,
  .yas-menuf-item-qaun button:hover span:before{
    background: #fff;
  }

  .yas-menuf-item-qaun button:active span,
  .yas-menuf-item-qaun button:focus span,
  .yas-menuf-item-qaun button:focus-visible span,
  .yas-menuf-item-qaun button:focus-within span{
    background-color: #B8B201;
  }
  .yas-menuf-item-qaun button:active span:after,
  .yas-menuf-item-qaun button:active span:before{
    background: #181806;
  }
  
  .yas-menuf-item-qaun input{
    width: 50px;
    height: 32px;
    border: none;
    border-top: 1px solid #181806;
    border-bottom: 1px solid #181806;
    pointer-events: none;
    text-align: center;
    background: transparent;
    -moz-appearance: textfield;
    -webkit-appearance: none;
  }
  .yas-menuf-item-qaun input::-webkit-outer-spin-button, .yas-menuf-item-qaun input::-webkit-inner-spin-button {
    -webkit-appearance: none;
    margin: 0;
  }
  .yas-menuf-item-price{
    margin-left: auto;
    display: flex;
    align-self: end;
    color: #181806;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px;
    text-transform: uppercase;
  }
  .yas-menuf-cartbtn{
    margin-top: auto;
    padding: 16px 16px 3px;
    display: flex;
    flex-direction: column;
  }
  .yas-menuf-bottom-image{
    display: inline-flex;
  }
  .yas-menuf-cartbtn button{
    background: #181806;
    display: flex;
    height: 56px;
    padding: 16px 24px;
    justify-content: center;
    align-items: center;
    gap: 8px;
    align-self: stretch;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px; /* 150% */
    text-transform: uppercase;
    color: #fff;
    width: 100%;
    margin: 12px 0;
    cursor: not-allowed;
    border: none;
    outline: none;
  }
  .yas-menuf-cartbtn button:hover{
    background: #fff;
    color: #181806;
    border: 1px solid #181806;
  }
  .yas-menuf-cartbtn button:active{
   background: #B8B201;
  }
  .yas-menuf-vat-pr{
    display: flex;
    justify-content: space-between;
  }
  .yas-menuf-vat-pr span{
    color: #181806;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px; /* 150% */
    text-transform: uppercase;
  }

  body .overlay-bg{
    background: rgba(0, 0, 0, 20%);
    /* backdrop-filter: blur(15px);
    -webkit-backdrop-filter: blur(15px); */
    position: fixed;
    z-index: 3;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    display: block;
  }
  body .overlay-bg ~ .header-logo-fix{
    z-index: 1;
  }
  .section-header{
    mix-blend-mode: difference;
  }
  body .overlay-bg ~ .section-header{
    mix-blend-mode: unset;
  }
  .menu-btn-yas, .cart-btn-yas{
    color: #fff;
  }
  .yas-menuf-bottom-image img{
    width: 100%;
    object-fit: cover;
  }
 @media (max-width: 989px) {
    .yas-menuf-item-qaun button:hover span{
      background-color: #b8b201;
    }
    .yas-menuf-item-qaun button:hover span:after,
    .yas-menuf-item-qaun button:hover span:before{
      background: #181806;
    }
    .yas-menuf-item-qaun button:active span{
      background-color: #B8B201;
    }
    .yas-menuf-item-qaun button:active span:after,
    .yas-menuf-item-qaun button:active span:before{
      background: #181806;
    }
    .yas-menuf-cartbtn button:hover{
      background: #181806;
      color: #FFF;
      border: none;
    }
    .yas-menuf-cartbtn button:active{
      background: #B8B201;
      color: #FFF;
      border: 1px solid #181806;
    }
   
  }
  
  @media (max-width: 479px){
    .yas-menu-f{
      width: 100%;
      height: 100vh;
      right: 0;
      margin: 0;
    }
    .yas-menu-f-textr{
      max-height: 100%;
    }
    .yas-menu-f-minicart .cart-items{
       max-height: 70vh;
    }
    .yas-menu-close:hover span:after{
      background-color: #B8B201;
    }
    .yas-menu-close:hover span{
      color: #B8B201;
    }
    .yas-menuf-bottom a{
       padding: 11px 0px;
        white-space: nowrap;
    }
  }
 
</style>




