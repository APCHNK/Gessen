<div class="yas-menu-f" id="yas-menupop">
  <button class="yas-menu-close"><span>X close</span></button>
  <div class="yas-menuf-link">
    {% for link in section.settings.menu.links %}
      <a href="{{ link.url }}">{{ link.title }}</a>
    {% endfor %}
  </div>
  <div class="yas-menuf-bottom">
    {% for block in section.blocks %}
      <a href="#menu-text-p{{ forloop.index}}" data-name="{{ block.settings.label | downcase }}" class="menu-text-btn">{{ block.settings.label }}</a>
    {% endfor %}
  </div>
</div>

{% for block in section.blocks %}
<div class="yas-menu-f yas-menu-f-textr" id="menu-text-p{{ forloop.index}}">
  <div class="yas-menu-f-title">
     <span>{{ block.settings.heading }}</span> <button class="yas-menu-close"><span>X close</span></button>
  </div>
  <div class="yas-menuf-text">
    {{ block.settings.content }}
  </div>
</div>
 {% endfor %}

<script>
  function closePopAll(){
     $('.yas-menu-f').css('display', 'none');
     $('.overlay-bg').remove();
     $('body').css('overflow', 'unset');
  }
  $(function(){

    
    
    $('.menu-btn-yas').click(function(){
      closePopAll();
      $('body').prepend('<div class="overlay-bg"></div>');
      $('.overlay-bg').click(function(){
        closePopAll();
      })
      
      $('body').css('overflow', 'hidden');
      $('#yas-menupop').css('display', 'flex');
    })

    $('.menu-text-btn').click(function(){
      closePopAll();
      $('body').prepend('<div class="overlay-bg"></div>');
      
      $('.overlay-bg').click(function(){
        closePopAll();
      })
      
      $('body').css('overflow', 'hidden');
      let id = $(this).attr('href');
      console.log(id);
      $(id).css('display', 'flex');
      $(id + ' .yas-menuf-text').scrollTop(0)
      console.log($(id))
    })
    $('.yas-menu-close').click(function(){
      closePopAll();
    })

    $('.cart-btn-yas').click(function(){
      closePopAll();
      $('body').prepend('<div class="overlay-bg"></div>');
      $('.overlay-bg').click(function(){
        closePopAll();
        const emp = document.querySelectorAll('.emp')
    emp.forEach(emps => {
      emps.style.display = 'none'
    })
      })
      
      $('body').css('overflow', 'hidden');
      $('#yas-cartpop').css('display', 'flex');
    })
    
  })

  

</script>


{% schema %}
{
  "name": "header",
  "class": "section-header",
  "settings": [
    {
      "type": "image_picker",
      "id": "logo",
      "label": "Logo"
    },
    {
      "type": "link_list",
      "id": "menu",
      "label": "Menu"
    }
  ],
  "blocks": [
    {
      "type": "secondary_links",
      "name": "Secondary Links",
      "limit": 4,
      "settings": [
        {
          "type": "text",
          "id": "heading",
          "label": "Content Heading",
          "default": "Terms"
        },
        {
          "type": "text",
          "id": "label",
          "label": "URL Label",
          "default": "Terms"
        },
        {
          "type": "richtext",
          "id": "content",
          "label": "Content",
          "default": "<p>Lorem ipsum dolor sit amet consectetur. Sagittis at pellentesque venenatis habitasse massa. In ut aliquam enim non facilisis pretium tristique tristique integer.</p>"
        }
      ]
    }
  ]
}
{% endschema %}
